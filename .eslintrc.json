{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "quotes": "off",
    "implicit-arrow-linebreak": "off",
    "comma-dangle": "off",
    "no-tabs": "off",
    "no-plusplus": "off",
    "indent": ["error", "tab"]
  }
}
